{
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "minLength": 1
    },
    "profileId": {
      "type": "string",
      "minLength": 1
    },
    "courses": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "minLength": 1
          },
          "courseId": {
            "type": "string"
          },
          "courseName": {
            "type": "string"
          },
          "credits": {
            "type": "number",
            "minimum": 0
          },
          "grade": {
            "enum": ["A+", "A", "B", "C", "D", "P", "認", "履修中", "-"]
          },
          "year": {
            "type": "integer",
            "minimum": 1900,
            "maximum": 2100
          },
          "semester": {
            "enum": ["spring", "fall", "full"]
          },
          "category": {
            "enum": ["A", "B", "C"]
          },
          "isPassed": {
            "type": "boolean"
          },
          "isInProgress": {
            "type": "boolean"
          }
        },
        "required": [
          "id",
          "courseId",
          "courseName",
          "credits",
          "grade",
          "year",
          "semester",
          "category",
          "isPassed",
          "isInProgress"
        ]
      }
    },
    "importedAt": {
      "type": "string",
      "format": "date-time"
    },
    "updatedAt": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": ["id", "profileId", "courses", "importedAt", "updatedAt"],
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "履修データスキーマ（TWINSから取り込んだ成績情報）"
}
