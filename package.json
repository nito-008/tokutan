{
  "name": "example-with-tailwindcss",
  "type": "module",
  "scripts": {
    "dev": "vinxi dev",
    "build": "vinxi build",
    "start": "serve .output/public",
    "format": "biome format --write .",
    "lint": "biome lint --write .",
    "check": "biome check .",
    "fix": "biome check --write .",
    "typecheck": "tsgo --noEmit",
    "gen": "tsx scripts/generate-schemas.ts",
    "deploy": "wrangler pages deploy .output/public"
  },
  "dependencies": {
    "@kobalte/core": "^0.13.11",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.0",
    "@solidjs/start": "^1.1.0",
    "@thisbeyond/solid-dnd": "^0.7.5",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dexie": "^4.2.1",
    "lucide-solid": "^0.562.0",
    "solid-js": "^1.9.5",
    "solid-sonner": "^0.2.8",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "ts-pattern": "^5.9.0",
    "valibot": "^1.2.0",
    "vinxi": "^0.5.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@tailwindcss/vite": "^4.0.7",
    "@types/node": "^25.0.10",
    "@typescript/native-preview": "^7.0.0-dev.20260116.1",
    "@valibot/to-json-schema": "^1.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tailwindcss": "^4.0.7",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "wrangler": "^4.61.1"
  },
  "engines": {
    "node": ">=22"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "biome check --write"
    ]
  }
}
